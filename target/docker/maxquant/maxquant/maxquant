            ;;
        --ms_instrument)
            [ -n "$VIASH_PAR_MS_INSTRUMENT" ] && ViashError Bad arguments for option \'--ms_instrument\': \'$VIASH_PAR_MS_INSTRUMENT\' \& \'$2\' - you should provide exactly one argument for this option. && exit 1
