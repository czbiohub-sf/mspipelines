              VIASH_PAR_REFERENCE=$(ViashRemoveFlags "$1")
            else
              VIASH_PAR_REFERENCE="$VIASH_PAR_REFERENCE;"$(ViashRemoveFlags "$1")
            fi
            shift 1
            ;;
        --ref_identifier_rule)
            if [ -z "$VIASH_PAR_REF_IDENTIFIER_RULE" ]; then
              VIASH_PAR_REF_IDENTIFIER_RULE="$2"
            else
              VIASH_PAR_REF_IDENTIFIER_RULE="$VIASH_PAR_REF_IDENTIFIER_RULE;""$2"
            fi
            [ $# -lt 2 ] && ViashError Not enough arguments passed to --ref_identifier_rule. Use "--help" to get more information on the parameters. && exit 1
