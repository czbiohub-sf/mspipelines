  docker.enabled = true
  def viash_temp = System.getenv("VIASH_TEMP") ?: "/tmp/"
  docker.runOptions = "-i -v ${baseDir}:${baseDir} -v $viash_temp:$viash_temp"
  process.container = "dataintuitive/viash"
  params {
    rawbeans__input = "viash_no_value"
    rawbeans__output = "viash_no_value"
    rawbeans__batch = "no_default_value_configured"
    rawbeans__no_report = "no_default_value_configured"
    rawbeans__mass = "no_default_value_configured"
    id = ""
    testScript = ""
    testResources = [  ]
    rawbeans {
      name = "rawbeans"
      container = "qc_rawbeans"
      containerTag = "0.2.0"
      containerRegistry = "ghcr.io"
      containerOrganization = "czbiohub/mspipelines"
      command = "rawbeans"
      tests {
        isDefined = false
        testScript = "NA"
        testResources = [  ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.rawbeans__input}"
          example = "input.mzML"
          description = "One or more mzML files."
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.rawbeans__output}"
          example = "output_qc_dir"
          description = "An output directory to the QC results."
        }
        batch {
          name = "batch"
          otype = "--"
          required = false
          type = "boolean_true"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.rawbeans__batch}"
          description = "When specified, will generate one report per sample instead of one report for all samples."
        }
        no_report {
          name = "no_report"
          otype = "--"
          required = false
          type = "boolean_true"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.rawbeans__no_report}"
          description = "When specified, will only generate json and no report."
        }
        mass {
          name = "mass"
          otype = "--"
          required = false
          type = "double"
          direction = "Input"
          multiple = true
          multiple_sep = ":"
          value = "${params.rawbeans__mass}"
          description = "List of masses to follow. Example: '--mass 371.10124 --mass 445.12003' or '--mass 371.10124:445.12003'."
        }
      }
    }
  }